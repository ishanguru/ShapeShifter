
/* 
    Test that the return type for Intersect is Shape.
*/

Scene {

    Sphere sph1 = SPHERE; 
    Cube cu1 = CUBE; 
    Shape un1 = Intersect(sph1, cu1); // Okay 
    Sphere un2 = Intersect(sph1, cu1); //NOT OKAY
    Cone un3 = Intersect(un1, sph1); // NOT OKAY
    Cube un4 = Intersect(CUBE, CUBE); // NOT OKAY
    
}
